[user]
	name = Fredrik LÃ¶fgren
	email = fr3drik.l@gmail.com
[push]
	default = simple
	followTags = true
[pull]
	ff = only
[color]
	ui = auto
[color "branch"]
	current = green reverse
	local = green
	remote = yellow
[diff]
	tool = diffmerge
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	fb = checkout -b
	lg = log --color --graph --pretty=format:'%C(yellow)%h%Creset -%C(magenta)%d%Creset %C(cyan)%s%Creset (%cr) %Cgreen%aN%Creset' --abbrev-commit --branches
	lgp = log --color --pretty=format:'%C(yellow)%h%Creset -%Cgreen%d%Creset %s - %C(magenta)%aN%Creset' --abbrev-commit
[core]
	editor = nano
[rerere]
	enabled = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true